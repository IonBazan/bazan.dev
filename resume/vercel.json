{
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": ".*(curl|wget|HTTPie|Postman).*"
        }
      ],
      "destination": "/raw.txt"
    },
    {
      "source": "/",
      "destination": "/myindex.html"
    }
  ]
}
